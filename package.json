{
  "name": "with-typescript-eslint-jest",
  "author": "@erikdstock",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev -p 3500",
    "generate": "prisma generate",
    "postinstall": "prisma generate",
    "pre-migrate": "prisma migrate save --experimental",
    "migrate": "prisma migrate up --experimental && prisma generate",
    "build": "yarn migrate && next build",
    "start": "next start -p 3500",
    "studio": "prisma studio",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "serve": "serve -S out -l 3000",
    "prod": "next build && next start",
    "lint-eslint": "eslint . --ext ts --ext tsx",
    "lint": "yarn type-check && yarn lint-eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run type-check && yarn lint"
    }
  },
  "dependencies": {
    "@analytics/google-analytics": "^0.5.2",
    "@prisma/client": "^2.11.0",
    "analytics": "^0.5.5",
    "clsx": "^1.1.1",
    "dayjs": "^1.9.6",
    "md5": "^2.3.0",
    "next": "10.0.1",
    "next-pwa": "^3.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^3.11.0",
    "react-player": "^2.6.2",
    "react-portal": "^4.2.1",
    "sass": "^1.29.0",
    "styled-components": "^5.2.1",
    "use-analytics": "^0.0.5"
  },
  "devDependencies": {
    "@prisma/cli": "^2.11.0",
    "@types/md5": "^2.2.1",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.2.3",
    "lint-staged": "^10.5.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.2",
    "serve": "^11.3.2",
    "tailwindcss": "^1.9.6",
    "typescript": "^4.0.5",
    "typescript-plugin-css-modules": "^2.8.0"
  }
}